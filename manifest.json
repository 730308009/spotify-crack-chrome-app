{

   "background": {
      "scripts": [ "js/background.js" ]
      
   },
   "content_scripts": [
      {
        "matches": ["https://open.spotify.com/*"], 
        "css" : ["style.css"]
      }
  ],

   "description": "Spotify Crack!",
   "icons": {
      "128": "img/icon.png"
   },
   "manifest_version": 2,
   "name": "Spotify Crack",
   "permissions": [ "webRequest", "webRequestBlocking", "*://open.spotify.com/*", "*://audio-ak-spotify-com.akamaized.net/*", "http://*.scdn.co/*" ],
   "version": "1.0"
}
